{
  "table_name": "fixed_broadband.subscriber_profile",
  "description": "Fixed Broadband subscriber master data with service metrics and revenue indicators",
  "columns": {
    "Subs_ID": {
      "type": "VARCHAR",
      "description": "Unique subscriber identifier",
      "indexed": true
    },
    "Service_Number_Trimmed": {
      "type": "VARCHAR",
      "description": "Service phone/account number (trimmed)",
      "indexed": true
    },
    "Subscriber_Status": {
      "type": "VARCHAR",
      "description": "Current subscription status/lifecycle stage",
      "values": ["Active", "Suspended", "Deactivated"]
    },
    "Payment_Type": {
      "type": "VARCHAR",
      "description": "Payment model for subscription",
      "values": ["Prepaid", "Postpaid"]
    },
    "TTS_Total_Tickets": {
      "type": "INTEGER",
      "description": "Total support tickets created for service issues",
      "aggregates": ["SUM", "AVG", "MAX"]
    },
    "Count_Of_All_Outage": {
      "type": "INTEGER",
      "description": "Total count of service outages experienced",
      "aggregates": ["SUM", "AVG", "MAX"]
    },
    "Current_Package_Name": {
      "type": "VARCHAR",
      "description": "Current service package subscription",
      "values": ["Standard", "Premium", "Enterprise"]
    },
    "Total_RPU": {
      "type": "NUMERIC",
      "format": "currency",
      "description": "Total Revenue Per User including all services (EGP)",
      "aggregates": ["AVG", "SUM", "SEGMENT"]
    },
    "Tenure_Days": {
      "type": "INTEGER",
      "description": "Number of days as active subscriber",
      "aggregates": ["AVG", "HISTOGRAM"]
    },
    "Gender": {
      "type": "VARCHAR",
      "description": "Subscriber gender",
      "values": ["M", "F", "U"]
    },
    "Has_GSM": {
      "type": "VARCHAR",
      "description": "Whether subscriber has GSM/mobile bundled service",
      "values": ["Yes", "No"]
    },
    "GOV": {
      "type": "VARCHAR",
      "description": "Governorate/Geographic region code",
      "values": ["Cairo", "Giza", "Alexandria", "Qalyubia", "Others"],
      "indexed": true
    }
  },
  "sample_query": "SELECT Subs_ID, Subscriber_Status, Total_RPU, Current_Package_Name FROM fixed_broadband.subscriber_profile WHERE GOV='Cairo' AND Subscriber_Status='Active'",
  "refresh_frequency": "daily",
  "record_count_approx": 5000000,
  "data_quality_notes": "Rolling refresh with daily transactions from broadband service system"
}
